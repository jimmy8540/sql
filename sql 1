use jimmy 
create database peoples
create table1 department(
PK dept name varchar(20)
   building budget int(20)
   )ENGINE=InnoDB DEFAULT CHARSET=utf8;
create table2 student(
PK ID varchar(20)
FK1 name varchar(10)
    sex char(1)
    age int(3)
    emotion_state varchar(10)
    dept_name varchar(10)
    )ENGINE=InnoDB DEFAULT CHARSET=utf8;
create table3 exam(
    student_ID
    exam_name
    grade varchar(10)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;
